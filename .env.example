# Base URL of the deployed site (include protocol, no trailing slash)
NEXT_PUBLIC_SITE_URL=https://www.signqa.dev

# Domain configured in Plausible analytics
# Defaults to "signqa.dev" if unset
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=signqa.dev

# Optional endpoint for receiving Core Web Vitals payloads
# Defaults to the built-in /api/web-vitals route
NEXT_PUBLIC_ANALYTICS_ENDPOINT=
